{
  "name": "POAPScan",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "watch": {
    "build": "src/index.ts"
  },
  "scripts": {
    "clean": "yarn run clean-build-steps && rm -rf dist",
    "tsc": "./node_modules/.bin/tsc",
    "babel": "./node_modules/.bin/babel build-tsc --out-dir build-babel --source-maps",
    "webpack": "webpack && cp -r public/* dist",
    "clean-build-steps": "rm -rf build-tsc build-babel",
    "firebase": "firebase deploy --project poapscan",
    "build": "yarn run clean && yarn run tsc && yarn run babel && yarn run webpack && yarn run clean-build-steps && yarn run firebase",
    "watch": "npm-watch"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "copy-webpack-plugin": "^4.6.0",
    "typescript": "^3.2.2",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.4",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "bs58check": "^2.1.2",
    "ethereum-ens": "^0.7.4",
    "file-loader": "^3.0.1",
    "mustache": "^3.0.1",
    "npm-watch": "^0.5.0",
    "web3": "^1.0.0-beta.46"
  }
}
